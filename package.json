{
  "name": "phenome-component-github-repos",
  "version": "1.0.0",
  "description": "Demo Phenome component to fetch GitHub repos for selected user",
  "scripts": {
    "serve-demo": "http-server ./demo/ -o -c 1 -a localhost",
