    "react": "^16.4.2",
    "react-dom": "^16.4.2",
    "rollup": "^0.63.5",
    "rollup-plugin-buble": "^0.19.2",
    "rollup-plugin-commonjs": "^9.1.4",
    "rollup-plugin-node-resolve": "^3.3.0",
    "rollup-plugin-replace": "^2.0.0",
    "vue": "^2.5.17"
  },
  "repository": {
    "type": "git",
